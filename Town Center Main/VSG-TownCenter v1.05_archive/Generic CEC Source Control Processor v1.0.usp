/*******************************************************************************************
  Event Handlers
  (Uncomment and declare additional event handlers as needed)
*******************************************************************************************/

CHANGE From_Device
{
	aAddressIn = byte(From_Device, 1);
	if(aAddressIn = aAddressRev)
	{
		if(byte(From_Device, 2) = 0x0090) // Power Status
		{
			PowerStatus = byte(From_Device, 3);
		}
		else if(byte(From_Device, 2) = 0x0047)	// OSD Name
		{
			sOSDNameTemp = mid(From_Device, 3, len(From_Device) - 2);
			if (sOSDNameTemp <> sOSDNameOut)
			{
				sOSDNameOut = sOSDNameTemp;
				OSDName = sOSDNameOut;
            }
		}
	}
}

/*******************************************************************************************
  Main()
  Uncomment and place one-time startup code here
  (This code will get called when the system starts up)
*******************************************************************************************/

Function Main()
{
	aAddressRev = ((byte(Address, 1) << 4) + (byte(Address, 1) >> 4)) & 0x00FF;
	clearbuffer(sOSDNameTemp);
}
